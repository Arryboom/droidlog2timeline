<?xml version="1.0" encoding="ASCII"?>
<root>
<database id="databases/calendar.db">
<table id="Events">
<column id="title">title</column>
<column id="start">dtstart</column>
<column id="end">dtend</column>
<column id="description">eventLocation</column>
<column id="description">description</column>
<column id="description">eventTimezone</column>
<column id="description">organizer</column>
<icon desc="Calendar events">calendar.png</icon>
<insert id="caption">Event planned in calendar</insert>
<filter static="calender events"
columns="title;eventLocation;description;organizer" />
</table>
</database>
</root>
